generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Student {
  id        String     @id @default(uuid())
  name      String
  surname   String
  email     String
  age       Int
  weight    Float
  height    Float
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@map("students")
}
